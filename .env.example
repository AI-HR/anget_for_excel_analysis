# .env.example
# 复制此文件为.env并填入你的实际配置

# OpenAI API配置
OPENAI_API_KEY=your_openai_api_key_here
MODEL_NAME=gpt-3.5-turbo  # 或其他支持的模型
MODEL_BASE_URL=https://api.openai.com/v1  # 默认OpenAI API地址，如果使用其他API，请修改

# 输出目录配置
OUTPUT_DIR=./output  # 相对路径或绝对路径均可

LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=your_langchain_api_key_here
LANGCHAIN_PROJECT=excel_analysis_project

# 应用配置
DEBUG=true
LOG_LEVEL=INFO